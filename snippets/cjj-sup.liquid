<style>
	.cjj_sup svg{
		width: 30px;
		height: 30px;
	}
	.cjj_sup{
		overflow: hidden;
		background-color: red;
		display: inline-block;
		padding: 0 2px 10px;
		position: relative;
	}
	.cjj_sup .sup-top{
		display: block;
		margin-top: -10px;
	}
	.cjj_sup .sup-content{
		display: block;
		margin-top: -10px;
	}
	.cjj_sup:after{
		content: '';
		width: 30px;
		height: 30px;
		background-color: #fff;
		position: absolute;
		bottom: -20px;
		right: 7px;
		transform: rotate(58deg);
	}
	.cjj_sup:before{
		content: '';
		width: 30px;
		height: 30px;
		background-color: #FFFFFF;
		position: absolute;
		bottom: -20px;
		left: 7px;
		transform: rotate(-58deg);
	}
</style>

